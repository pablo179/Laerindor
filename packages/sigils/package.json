{
  "name": "@laerindor/sigils",
  "version": "0.0.0",
  "main": "src/index.js",
  "types": "src/index.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "dev": "tsc -w",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./history": {
      "types": "./dist/history.d.ts",
      "default": "./dist/history.js"
    },
    "./project": {
      "types": "./dist/project.d.ts",
      "default": "./dist/project.js"
    },
    "./history.css": "./src/history.css",
    "./project.css": "./src/project.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "dependencies": {
    "@repo/typescript-config": "",
    "eslint-plugin-prettier": "^5.5.3",
    "typescript": "5.8.3"
  }
}
